# Brake thermal model with pad-disk radiation coupling and external heat inputs
#
# This component models the thermal behavior of a brake system consisting of:
# - Brake disk: Primary thermal mass with convection and radiation cooling to ambient
# - Brake pad: Secondary thermal mass with convection cooling only
# - Radiation coupling: Heat exchange between pad and disk via radiation
#
# Heat generation is provided externally via thermal ports for flexible coupling
# to mechanical friction models.
#
# Energy balance equations:
# Disk: C_disk * dT_disk/dt = Q_heat_disk - Q_conv_disk - Q_rad_disk + Q_rad_pad_disk
# Pad:  C_pad * dT_pad/dt = Q_heat_pad - Q_conv_pad - Q_rad_pad_disk
#
component BrakeThermal
  # === External heat input ports ===
  # Heat input to disk (from friction)
  heat_disk = Node() {^heat_disk}
  # Heat input to pad (from friction)
  heat_pad = Node() {^heat_pad}
  # === Speed inputs ===
  # Vehicle speed input [m/s]
  vehicle_speed = RealInput() {^vehicle_speed}
  # Wheel angular speed input [rad/s]
  wheel_speed = RealInput() {^wheel_speed}
  # === Thermal masses ===
  disk_mass = ThermalComponents.HeatCapacitor(C = C_disk, T0 = T_disk_init) {^disk_mass}
  pad_mass = ThermalComponents.HeatCapacitor(C = C_pad, T0 = T_pad_init) {^pad_mass}
  # === Convective cooling ===
  disk_convection = ThermalComponents.Convection() {^disk_convection}
  pad_convection = ThermalComponents.Convection() {^pad_convection}
  # === Radiative cooling (disk to ambient only) ===
  disk_radiation = ThermalComponents.BodyRadiation(Gr = epsilon_disk * A_disk) {^disk_radiation}
  # === Pad-disk radiation coupling ===
  pad_disk_radiation = ThermalComponents.BodyRadiation(Gr = G_rad_pad_disk) {^pad_disk_radiation}
  # === Ambient temperature boundary ===
  ambient = ThermalComponents.FixedTemperature(T = T_ambient) {^ambient}
  # === Convection coefficient calculations ===
  # Disk convection: h_disk = h_disk_base + k_disk_speed * |wheel_speed|
  wheel_speed_gain = BlockComponents.Gain(k = k_disk_speed) {^wheel_speed_gain}
  disk_h_base_const = BlockComponents.Constant(k = h_disk_base) {^disk_h_base_const}
  disk_h_calc = BlockComponents.Add(k1 = 1.0, k2 = 1.0) {^disk_h_calc}
  # Pad convection: h_pad = h_pad_base + k_pad_speed * |vehicle_speed|
  vehicle_speed_gain = BlockComponents.Gain(k = k_pad_speed) {^vehicle_speed_gain}
  pad_h_base_const = BlockComponents.Constant(k = h_pad_base) {^pad_h_base_const}
  pad_h_calc = BlockComponents.Add(k1 = 1.0, k2 = 1.0) {^pad_h_calc}
  # Convert to thermal conductances (h * A)
  disk_conductance = BlockComponents.Product() {^disk_conductance}
  pad_conductance = BlockComponents.Product() {^pad_conductance}
  # Constants for surface areas
  disk_area_const = BlockComponents.Constant(k = A_disk) {^disk_area_const}
  pad_area_const = BlockComponents.Constant(k = A_pad) {^pad_area_const}
  # === Thermal parameters ===
  # Disk thermal capacity [J/K]
  parameter C_disk::HeatCapacity = 4000
  # Pad thermal capacity [J/K]
  parameter C_pad::HeatCapacity = 1000
  # Disk initial temperature [K]
  parameter T_disk_init::Temperature = 293.15
  # Pad initial temperature [K]
  parameter T_pad_init::Temperature = 293.15
  # Ambient temperature [K]
  parameter T_ambient::Temperature = 293.15
  # === Heat transfer parameters ===
  # Base disk convection coefficient [W/(m2.K)]
  parameter h_disk_base::CoefficientOfHeatTransfer = 20.0
  # Base pad convection coefficient [W/(m2.K)]
  parameter h_pad_base::CoefficientOfHeatTransfer = 15.0
  # Disk convection enhancement factor [W.s/(m3.K)]
  parameter k_disk_speed::Real = 3.0
  # Pad convection enhancement factor [W.s/(m3.K)]
  parameter k_pad_speed::Real = 4.0
  # Disk surface area for convection [m2]
  parameter A_disk::Area = 0.15
  # Pad surface area for convection [m2]
  parameter A_pad::Area = 0.032
  # Disk emissivity [-]
  parameter epsilon_disk::Real = 0.75
  # Radiative conductance for pad-disk coupling [m2]
  parameter G_rad_pad_disk::Area = 0.012
relations
  # Connect heat inputs to thermal masses
  connect(heat_disk, disk_mass.node) {^id22}
  connect(heat_pad, pad_mass.node) {^id23}
  # Build disk convection coefficient: h_disk = h_disk_base + k_disk_speed * wheel_speed
  connect(wheel_speed, wheel_speed_gain.u) {^id24}
  connect(disk_h_base_const.y, disk_h_calc.u1) {^id25}
  connect(wheel_speed_gain.y, disk_h_calc.u2) {^id26}
  # Build pad convection coefficient: h_pad = h_pad_base + k_pad_speed * vehicle_speed
  connect(vehicle_speed, vehicle_speed_gain.u) {^id27}
  connect(pad_h_base_const.y, pad_h_calc.u1) {^id28}
  connect(vehicle_speed_gain.y, pad_h_calc.u2) {^id29}
  # Calculate thermal conductances (h * A)
  connect(disk_h_calc.y, disk_conductance.u1) {^id30}
  connect(disk_area_const.y, disk_conductance.u2) {^id31}
  connect(pad_h_calc.y, pad_conductance.u1) {^id32}
  connect(pad_area_const.y, pad_conductance.u2) {^id33}
  # Connect convective cooling
  connect(disk_mass.node, disk_convection.solid) {^id34}
  connect(pad_mass.node, pad_convection.solid) {^id35}
  connect(disk_convection.fluid, ambient.node) {^id36}
  connect(pad_convection.fluid, ambient.node) {^id37}
  # Connect disk radiation to ambient
  connect(disk_mass.node, disk_radiation.node_a) {^id38}
  connect(disk_radiation.node_b, ambient.node) {^id39}
  # Connect pad-disk radiation
  connect(pad_mass.node, pad_disk_radiation.node_a) {^id40}
  connect(disk_mass.node, pad_disk_radiation.node_b) {^id41}
  # Connect convection coefficient signals
  connect(disk_conductance.y, disk_convection.Gc) {^id42}
  connect(pad_conductance.y, pad_convection.Gc) {^id43}
