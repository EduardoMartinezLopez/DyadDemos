# Test component for the refined brake thermal model with speed inputs
test component BrakeThermalTest_Constant
  # Brake thermal model instance
  brake_thermal = BrakeThermal() {^brake_thermal}
  # Fixed heat inputs to simulate friction
  disk_heat_input = ThermalComponents.FixedHeatFlow(Q = Q_disk) {^disk_heat_input}
  pad_heat_input = ThermalComponents.FixedHeatFlow(Q = Q_pad) {^pad_heat_input}
  # Speed signal sources
  vehicle_speed_source = BlockComponents.Constant(k = vehicle_speed) {^vehicle_speed_source}
  wheel_speed_source = BlockComponents.Constant(k = wheel_speed) {^wheel_speed_source}
  # Heat flow to disk
  parameter Q_disk::HeatFlowRate = 9300
  # Heat flow to disk
  parameter Q_pad::HeatFlowRate = 700
  # Vehicle speed
  parameter vehicle_speed::Velocity = 25
  # Wheel speed
  parameter wheel_speed::AngularVelocity = 83
relations
  # Connect heat inputs
  connect(disk_heat_input.node, brake_thermal.heat_disk) {^id6}
  connect(pad_heat_input.node, brake_thermal.heat_pad) {^id7}
  # Connect speed inputs
  connect(vehicle_speed_source.y, brake_thermal.vehicle_speed) {^id8}
  connect(wheel_speed_source.y, brake_thermal.wheel_speed) {^id9}
