# Transient analysis of brake thermal model - 1800s simulation
analysis BrakeThermalAnalysis_Constant
  extends TransientAnalysis(stop = 1800.0, reltol = 1e-6, abstol = 1e-8)
  model = BrakeThermalTest_Constant(Q_disk = Q_disk, Q_pad = Q_pad, vehicle_speed = vehicle_speed, wheel_speed = wheel_speed)
  # Heat flow to disk
  parameter Q_disk::HeatFlowRate = 9300
  # Heat flow to disk
  parameter Q_pad::HeatFlowRate = 700
  # Vehicle speed
  parameter vehicle_speed::Velocity = 25
  # Wheel speed
  parameter wheel_speed::AngularVelocity = 83
end

# Test component for brake thermal model with transient heat application (1500s on, then off)
test component BrakeThermalTest_Step
  # Brake thermal model instance
  brake_thermal = BrakeThermal() {^brake_thermal}
  # Heat input sources with time-dependent behavior
  # Disk heat: 9300W for first 1500s, then 0W
  disk_heat_step = BlockComponents.Step(height = -9300, offset = 9300, start_time = 1500.0) {^disk_heat_step}
  disk_heat_input = ThermalComponents.PrescribedHeatFlow() {^disk_heat_input}
  # Pad heat: 700W for first 1500s, then 0W
  pad_heat_step = BlockComponents.Step(height = -700, offset = 700, start_time = 1500.0) {^pad_heat_step}
  pad_heat_input = ThermalComponents.PrescribedHeatFlow() {^pad_heat_input}
  # Speed signal sources (same as original test)
  vehicle_speed_source = BlockComponents.Constant(k = 6.94) {^vehicle_speed_source}
  wheel_speed_source = BlockComponents.Constant(k = 64.8) {^wheel_speed_source}
relations
  # Connect heat step signals to prescribed heat flows
  connect(disk_heat_step.y, disk_heat_input.Q) {^id8}
  connect(pad_heat_step.y, pad_heat_input.Q) {^id9}
  # Connect heat flows to thermal model
  connect(disk_heat_input.node, brake_thermal.heat_disk) {^id10}
  connect(pad_heat_input.node, brake_thermal.heat_pad) {^id11}
  # Connect speed inputs
  connect(vehicle_speed_source.y, brake_thermal.vehicle_speed) {^id12}
  connect(wheel_speed_source.y, brake_thermal.wheel_speed) {^id13}
metadata {
  "Dyad": {
    "tests": {
      "case1": {
        "stop": 5000,
