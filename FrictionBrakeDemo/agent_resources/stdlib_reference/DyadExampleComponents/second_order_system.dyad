component SecondOrderSystem
  xdd = RealInput() {^xdd}
  integrator1 = BlockComponents.Integrator() {^integrator1}
  integrator2 = BlockComponents.Integrator() {^integrator2}
  gain1 = BlockComponents.Gain(k = k1) {^gain1}
  gain2 = BlockComponents.Gain(k = k2) {^gain2}
  gain3 = BlockComponents.Gain(k = k3) {^gain3}
  sum = BlockComponents.Add3() {^sum}
  parameter k1::Real
  parameter k2::Real
  parameter k3::Real
relations
  connect(xdd, integrator1.u, gain1.u) {^id8}
  connect(integrator1.y, integrator2.u, gain2.u) {^id9}
  connect(integrator2.y, gain3.u) {^id10}
  connect(gain1.y, sum.u3) {^id11}
  connect(gain2.y, sum.u2) {^id12}
  connect(gain3.y, sum.u1) {^id13}
